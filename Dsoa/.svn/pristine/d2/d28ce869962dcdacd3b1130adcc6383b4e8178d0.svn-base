}.list_all {
	border: 1px solid #57B3DF;
}
.td_th {
	border-top-width: 1px;
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;
	border-top-style: none;
	border-right-style: none;
	border-bottom-style: solid;
	border-left-style: none;
	border-top-color: #57B3DF;
	border-right-color: #57B3DF;
	border-bottom-color: #57B3DF;
	border-left-color: #57B3DF;
	padding-top: 5px;
}
.tab {
	border-top-width: 1px;
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;
	border-top-style: solid;
	border-right-style: solid;
	border-bottom-style: none;
	border-left-style: solid;
	border-top-color: #57B3DF;
	border-right-color: #57B3DF;
	border-bottom-color: #57B3DF;
	border-left-color: #57B3DF;
	font-weight: bold;
	color: #000000;
	background-image: url(../images/icon/dot.gif);
	background-repeat: no-repeat;
	background-position: left center;
	padding-left: 31px;
	font-size: 14px;
	padding-top: 7px;
	cursor: hand;
}
.tab_down {
	background-color: #D7F0FF;
	border-top-width: 1px;
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;
	border-top-style: solid;
	border-right-style: solid;
	border-bottom-style: none;
	border-left-style: solid;
	border-top-color: #57B3DF;
	border-right-color: #57B3DF;
	border-bottom-color: #57B3DF;
	border-left-color: #57B3DF;
	font-weight: bold;
	color: #006BBD;
	background-image: url(../images/icon/dot_down.gif);
	background-repeat: no-repeat;
	background-position: left center;
	padding-left: 31px;
	font-size: 14px;
	padding-top: 7px;
}
.tab_right {
	border-top-width: 1px;
	border-right-width: 1px;
	border-bottom-width: 1px;
	border-left-width: 1px;
	border-top-style: none;
	border-right-style: none;
	border-bottom-style: none;
	border-left-style: solid;
	border-top-color: #57B3DF;
	border-right-color: #57B3DF;
	border-bottom-color: #57B3DF;
	border-left-color: #57B3DF;
}
